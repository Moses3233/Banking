package com.myBank.main;

import java.util.Scanner;

public class bankAppMain {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
	
		System.out.println("Welcome to the Bank! Do you already have an account with us?\nType \"1\" for yes or \"2\" for no.\nPress\"3\" to exit now");
		int welcomeOption = Integer.parseInt(sc.nextLine()) ;
		
		switch(welcomeOption) {
		
		case 1:
			break;
			
		case 2:
			break;
			
		case 3:
			System.exit(0);
			break;
		default:
			System.out.println("Not a valid option. Try again.");
		
		
		}
	}

}
